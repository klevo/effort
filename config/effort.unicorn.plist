<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>KeepAlive</key>
  <true/>
  <key>Label</key>
  <string>effort.unicorn</string>
  <key>ProgramArguments</key>
  <array>
    <string>/usr/bin/env</string>
    <string>sh</string>
    <string>-c</string>
    <string>/Users/klevo/.rbenv/shims/bundle exec unicorn -E production -c /www/effort/config/unicorn.rb</string>
  </array>
  <key>RunAtLoad</key>
  <true/>
  <key>LaunchOnlyOnce</key>
  <true/>
  <key>UserName</key>
  <string>klevo</string>
  <key>WorkingDirectory</key>
  <string>/www/effort</string>
  <key>StandardErrorPath</key>
  <string>/www/effort/log/effort.unicorn.launchd.log</string>
</dict>
</plist>
