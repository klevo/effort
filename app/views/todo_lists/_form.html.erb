<%= form_for [:project, TodoList.new], remote: true, format: :js do |f| %>
  <h3>Adding new todo list</h3>
  <%= f.text_field :name, data: { behavior: 'enter_submits' } %>

  <div class="actions">
    <%= f.submit "Add this list" %>
    <span class="or_cancel">or <%= link_to 'Cancel', '#', data: { behavior: 'cancel_add_todo_list_form' } %></span>
  </div>
<% end %>
